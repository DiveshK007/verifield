# VeriField Frontend Environment Variables
# Copy this file to .env.local and fill in your values

# Smart Contract Addresses (update these after deployment)
NEXT_PUBLIC_DATANFT_ADDRESS=0x...
NEXT_PUBLIC_MARKETPLACE_ADDRESS=0x...

# Blockchain Configuration
NEXT_PUBLIC_CHAIN_ID=31337
NEXT_PUBLIC_RPC_URL=http://127.0.0.1:8545

# App Configuration
NEXT_PUBLIC_APP_NAME=VeriField
NEXT_PUBLIC_STORAGE_GATEWAY=https://ipfs.io/ipfs/

# AI Features (optional)
NEXT_PUBLIC_AI_MODE=on
OPENAI_API_KEY=your_openai_api_key_here
